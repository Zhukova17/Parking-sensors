        <Image Margin="280,119,280,460" Source="/0,3 - 0,5 metra.png" Stretch="Fill"/>
        <Image Margin="240,99,240,480" Source="/0,5 - 1 metra.png" Stretch="Fill"/>
        <Image Margin="320,424,320,155" Source="/0,1 - 0,3 metra.png" Stretch="Fill"/>
        <Image Margin="280,444,280,135" Source="/0,3 - 0,5 metra.png" Stretch="Fill"/>
